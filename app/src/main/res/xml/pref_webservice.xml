<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
                  android:key="pref_webservice"
                  android:title="@string/pref_header_asset_control_conf">
    <PreferenceCategory android:title="@string/connection">
        <Preference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_network_check"
                android:key="ac_test"
                android:title="@string/test"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/auto_configuration">
        <Preference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_qrcode"
                android:key="ac_qr_code"
                android:title="@string/generate_configuration_qr_code"/>
        <Preference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_qrcode_scan"
                android:key="scan_config_code"
                android:summary="@string/if_you_have_a_configuration_barcode_you_can_capture_it_here"
                android:title="@string/capture_configuration_barcode"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/assetcontrol_webservice">
        <EditTextPreference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_server"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_server"
                android:maxLines="1"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/server"/>
        <EditTextPreference
                android:icon="@drawable/ic_namespace"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_namespace"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/namespace"/>
        <EditTextPreference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_user"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_user"
                android:maxLines="1"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/webservice_user"/>
        <EditTextPreference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_password"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_pass"
                android:maxLines="1"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/webservice_password"/>
        <EditTextPreference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_user_ac"
                android:inputType="textVisiblePassword"
                android:key="ac_user"
                android:maxLines="1"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/ac_user"/>
        <EditTextPreference
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:icon="@drawable/ic_password"
                android:inputType="textVisiblePassword"
                android:key="ac_pass"
                android:maxLines="1"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/ac_password"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/proxy">
        <SwitchPreference
                android:defaultValue="false"
                android:icon="@drawable/ic_proxy"
                android:key="ac_ws_use_proxy"
                android:title="@string/use_proxy"/>
        <EditTextPreference
                android:icon="@drawable/ic_cloud"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_proxy"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/proxy"/>
        <EditTextPreference
                android:icon="@drawable/ic_port"
                android:inputType="number"
                android:key="ac_ws_proxy_port"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/port"/>
        <EditTextPreference
                android:icon="@drawable/ic_user"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_proxy_user"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/user"/>
        <EditTextPreference
                android:icon="@drawable/ic_password"
                android:inputType="textVisiblePassword"
                android:key="ac_ws_proxy_pass"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:title="@string/password"/>
    </PreferenceCategory>
</PreferenceScreen>